{
  "expo": {
    "name": "react-native-pmt",
    "slug": "react-native-pmt",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./src/assets/images/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/footer/Mask_group.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.reactnativepmt"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/footer/Mask_group.png",
        "backgroundColor": "#000000"
      },
      "package": "com.anonymous.reactnativepmt"
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#000000"
    },
    "androidNavigationBar": {
      "backgroundColor": "#000000",
      "barStyle": "light-content"
    }
  }
}
